            "patterns": [".git", ".root", ".project"],
            "bottomUp": true
        }
    },
    "explorer.root.strategies": [
        "custom:vcs",
        "custom:vcs-r",
        "workspace",
        "cwd",
        "sourceBuffer",
        "reveal"
