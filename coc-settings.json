        "--clang-tidy",
        "--clang-tidy-checks=performance-*,bugprone-*",
        "--completion-style=detailed",
        "--header-insertion=never",
        "--pch-storage=disk"
    ],
    "signature.target": "float",
    "yank.enableCompletion":false,
