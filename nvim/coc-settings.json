        "struct": "󰆧",
        "class": "",
        "interface": "",
        "text": "󰉿",
        "enum": "",
        "enumMember": "",
        "module": "",
        "color": "󰏘",
        "property": "󰜢",
        "field": "",
        "unit": "󰑭",
        "event": "",
        "file": "󰈙",
        "folder": "󰉋",
        "snippet": "",
        "typeParameter": "",
        "constructor": "",
        "default": "󰏿"
