    "diagnostic.virtualText":true,
    "diagnostic.checkCurrentLine": false,
    "diagnostic.virtualTextPrefix":"\uf188 ",
    "diagnostic.virtualTextLevel": "warning",
    "diagnostic.enable":false,
    "diagnostic.messageTarget": "float",
    "diagnostic.messageLevel": "warning",
    "_comment": "disable diagnostic float window",
    "diagnostic.enableMessage": "never",
    "signature.target": "float",
    "signature.hideOnTextChange": true,
    "coc.preferences.extensionUpdateCheck":"weekly",
    "coc.preferences.currentFunctionSymbolAutoUpdate":true,
    "coc.preferences.messageLevel": "error",
    "coc.source.around.enable": false,
    "snippets.autoTrigger": true,
    "snippets.ultisnips.enable": false,
    "snippets.priority": 100,
    "json.enable": true,
    "inlayHint.enable": false,
    "clangd.enabled": true,
    "clangd.disableSnippetCompletion": false,
    "clangd.disableDiagnostics": false,
    "clangd.arguments": [
        "--background-index",
        "--query-driver=/usr/bin/clang++",
        "--clang-tidy",
        "--clang-tidy-checks=performance-*,bugprone-*",
        "--completion-style=detailed",
        "--header-insertion=never",
        "--pch-storage=disk"
    ],
